{
  "version": "0.0.1",
  "name": "efureev/laravel-trees",
  "authors": [
    {
      "name": "Eugene Fureev",
      "email": "fureev@gmail.com"
    }
  ],
  "require": {
    "illuminate/database": "~5.6",
    "illuminate/events": "~5.6",
    "efureev/support": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "fzaninotto/faker": "^1.8",
    "phpstan/phpstan": "^0.10.3",
    "symfony/var-dumper" : "*"
  },
  "autoload": {
    "psr-4": {
      "Fureev\\Trees\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fureev\\Trees\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fureev\\Trees\\NestedSetServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "@php ./vendor/bin/phpunit --no-coverage --testdox",
    "test-cover": "@php ./vendor/bin/phpunit --coverage-text",
    "phpstan": "@php ./vendor/bin/phpstan analyze --ansi --level=max ./app"
  }
}
