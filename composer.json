{
  "name": "efureev/laravel-trees",
  "description": "Multi-Tree structures for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Fureev",
      "email": "fureev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/database": "^8.38",
    "illuminate/events": "^8.38",
    "efureev/support": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.17",
    "fakerphp/faker": "^1.14",
    "symfony/var-dumper": "^5.2",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Fureev\\Trees\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fureev\\Trees\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpcs": "@php ./vendor/bin/phpcs",
    "cs-fix": "@php ./vendor/bin/phpcbf",
    "test": "@php ./vendor/bin/phpunit --no-coverage --testdox",
    "test-cover": "@php ./vendor/bin/phpunit --coverage-text",
    "phpstan": "@php ./vendor/bin/phpstan analyze --ansi"
  }
}
